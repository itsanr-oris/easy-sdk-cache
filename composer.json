{
    "name": "f-oris/easy-sdk-cache",
    "description": "Easy sdk cache component.",
    "license": "MIT",
    "authors": [
        {
            "name": "F.oris",
            "email": "us@f-oris.me"
        }
    ],
    "require": {
        "php": ">=5.5",
        "f-oris/easy-cache": "^0.5"
    },
    "require-dev": {
        "f-oris/easy-sdk-develop": "^0.5"
    },
    "autoload": {
        "psr-4": {
            "Foris\\Easy\\Sdk\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Foris\\Easy\\Sdk\\Cache\\Tests\\": "tests/"
        }
    },
    "extra": {
        "easy-sdk": {
            "providers": [
                "Foris\\Easy\\Sdk\\Cache\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
